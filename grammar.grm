use super::Ast;

$whitespace: () = [
    /\s/,
    comment,
];

comment: () = [
    "//" /.*$/ => (),
    "/*" rest_of_multiline_comment => (),
];

rest_of_multiline_comment: () = [
    "/*" rest_of_multiline_comment rest_of_multiline_comment => (),
    "*/" => (),
    /./ rest_of_multiline_comment => (),
];

